<body ng-controller="mainController">
<div class="container">
    <div class="loginContainer">
        <div class="login-logo"><img src="images/logo.png" /></div>
        <form role="form" id="signupForm" name="signupForm">
            <div class="form-group">
                <div class="input-field"><input type="text" ng-model="forgotPasswordData.email" class="form-control text-center" placeholder="Enter Email"/></div>
                <div class="input-field"><input type="submit" class=" btn btn-default c-submit" value="Send" ng-click="emailForgotPassword()"/></div>
            </div>
        </form>
    </div>
</div>
</div>
