<body ng-controller="mainController">
<div class="container">
<div class="loginContainer">
	<div class="login-logo"><img src="images/logo.png" /></div>
    <form role="form" id="loginForm" name="loginForm">
    <div class="form-group">
    	<div class="input-field"><input type="text"  class="form-control" placeholder="User Name" name="userName" id="userName" ng-model="formData.userName"/></div>
        <div class="input-field"><input type="password"  class="form-control" placeholder="Password" name="password" id="password" ng-model="formData.password"/></div>
        <div class="input-field"><input type="submit" class=" btn btn-default c-submit" value="LOGIN" ng-click="login()"/></div>
        <div class="input-field"><a href="#" id="forgotPasswordBtn">Forgot Password</a>|<a href="#" id="newUserBtn" name="newUserBtn">Register</a></div>
    </div>
    </form>
    </div>
</div>
</div>
